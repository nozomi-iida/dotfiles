{
  "files.autoSave": "onFocusChange",
  "vim.useSystemClipboard": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.inlineSuggest.enabled": true,
  "leetcode.defaultLanguage": "rust",
  "leetcode.workspaceFolder": "/Users/iidanozomushi/.leetcode",
  "leetcode.hint.commandShortcut": false,
  "leetcode.hint.configWebviewMarkdown": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "go.toolsManagement.autoUpdate": true,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },
  "eslint.format.enable": true,
  "prettier.requireConfig": true,
  "nxConsole.showNodeVersionOnStartup": false,
  "compareFolders.excludeFilter": [
    "**/node_modules",
    "**/.next"
  ],
  "git.autofetch": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "github.copilot.editor.enableAutoCompletions": true,
  "vim.leader": ",",
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "p"
      ],
      "after": [
        "\"",
        "_",
        "d",
        "P"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "e",
        "f"
      ],
      "commands": [
        "workbench.files.action.showActiveFileInExplorer"
      ]
    },
    {
      "before": [
        "<leader>",
        "e",
        "e"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
        "p"
      ],
      "commands": [
        "copyRelativeFilePath"
      ]
    },
  ],
  "git.enableSmartCommit": true,
  "githubPullRequests.createOnPublishBranch": "never",
  "githubPullRequests.terminalLinksHandler": "github",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "git.confirmSync": false,
  "githubPullRequests.pullBranch": "never",
  "gitlens.ai.generateCommitMessage.customInstructions": "Semantic Commit MessageおよびCommit Message Guidelineに従い、日本語のコミットメッセージを1行で作成してください。",
  "database-client.autoSync": true,
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-3.5-turbo",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
}
